<form action="/dinosaurs" method="GET">


    <label for="nameFilter">Filter by Name:</label>
    <input type="text" name="nameFilter" id="nameFilter">
    <input type="submit"></input>






</form>

<ul>
    <%myDinos.forEach((dino, i)=>{%>
        <li>
            <%=dino.name%> is a <%=dino.type%> 
            <form action="/dinosaurs/<%=i%>/?_method=DELETE" method="post">
                <input type="submit" value="Delete">
            </form>
        </li>
    <%})%>
</ul>